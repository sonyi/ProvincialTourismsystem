<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TabHostTv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">@dimen/tabhost_tv_14</item>
        <item name="android:background">@android:color/white</item>
        <item name="android:textColor">@color/tabhost_tvbg</item>
    </style>

    <style name="provicia_tvstly">
        <item name="android:textColor">@color/titlebar_tv</item>
        <item name="android:textSize">@dimen/qingxin_tv10</item>
    </style>

    <style name="bottom_bnt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">@dimen/tabhost_tv_14</item>
        <item name="android:textColor">@color/tabhost_tvbg</item>
        <item name="android:onClick">bottoms</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="settings_item_style" parent="@android:style/Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/settings_item_bg</item>
        <item name="android:drawableRight">@drawable/more</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:padding">8dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_marginLeft">0.5dp</item>
        <item name="android:layout_marginRight">0.5dp</item>
    </style>

    <style name="map_bnt">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/map_tvbg</item>
        <item name="android:background">@color/map_toum</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:drawablePadding">-10dp</item>
        <item name="android:textSize">@dimen/tabhost_tv_14</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="map_viewbnt">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/map_toum</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginRight">6dp</item>
        <item name="android:textColor">@color/titlebar_bg</item>
        <item name="android:textSize">@dimen/qingxin_tv17</item>
    </style>
